    <script>
       // This is a "comment" written in JavaScript. It will not have any affect on your scripts.
       // You will write your JavaScript between this opening script tag and the closing script tag commented below.
       
       //It is a good practice to write a comment for each script that you write. Both to remind yourself of what it does and help other people identify what your code is doing if they read it later.
       
       /* ----------
        Comments may be written on a single line using "//"
        or span multiple lines like this comment does
        when using the star and slash method
        ---------- */
       
        // Use the space below to follow along with the lesson...
       function doNothing () {}
       function sayHello () {
           console.log('Hello!')
       }
       sayHello () 
       // when called like so it says Hello!
       
       function sayHelloToJared () {
           console.log("Hello, Jared!")
       }
       sayHelloToJared ()
       
       function sayHelloToJodi () {
           console.log("Hello, Jodi!")
       }
       sayHelloToJodi ()
       
       function doSomething(thing) {
           console.log(thing)
       }
       
       function sayHelloTo(firstName) {
           console.log(`Hello, ${firstName}!`)
       }
       
       function say(greeting, firstName) {
           console.log(`${greeting}, ${firstName}!`)
       }
       say("Goodbye", "Han")

       function add(x,y) {
           return x+ y
       }

       function say(greeting, firstName) {
          return `${greeting}, ${firstName}!`
       }
       
       function say(greeting, firstName) {
           console.log(`3720 to 1`)
           return `${greeting}, ${firstName}!`
       }
       say("Never", "tell me the odds")

       function sayFacts() {
           console.log("Destiny 2 is the best game")
       }
       sayfacts()

       function witchQueenWhen () {
           console.log("Febuary 22, 2022")
       }
       whichQueenWhen()
 
       // This is the closing script tag. Your JavaScript must be written above this tag.
    </script>
    
